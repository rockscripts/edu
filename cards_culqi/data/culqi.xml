<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data noupdate="1">

		<record id="payment_acquirer_culqi" model="payment.acquirer">
			<field name="name">Culqi</field>
			<field name="provider">culqi</field>
			<field name="image_128" type="base64" file="cards_culqi/static/description/icon.png" />
		</record>
		<record id="payment_journal_culqi_pen" model="account.journal">
			<field name="name">Culqi PEN</field>
			<field name="code">CQPEN</field>
			<field name="type">bank</field>
		</record>
		<record id="payment_journal_culqi_usd" model="account.journal">
			<field name="name">Culqi USD</field>
			<field name="code">CQUSD</field>
			<field name="type">bank</field>
		</record>
		<record id="payment_icon" model="payment.icon">
			<field name="name">CULQI</field>
			<field name="image" type="base64" file="cards_culqi/static/description/icon.png" />
		</record>

	</data>
</odoo>
